{% extends 'base.html.twig' %}

{% block title %}BrandModel{% endblock %}

{% block body %}
    <h1>BrandModel</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ brand_model.id }}</td>
            </tr>
            <tr>
                <th>Label</th>
                <td>{{ brand_model.label }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_brand_model_index') }}">back to list</a>

	{% if is_granted('ROLE_ADMIN') %}
        <a href="{{ path('app_brand_model_edit', {'id': brand_model.id}) }}">edit</a>
        {{ include('brand_model/_delete_form.html.twig') }}
	{% endif %}
{% endblock %}
